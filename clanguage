    }
   	while(fscanf(fp,"\n%s  \n%s  \n%s  \n%s",coustomer.name,coustomer.add,coustomer.email,coustomer.ph)!=EOF)
  	printf("\n\t\t\t\t\t\tNAME:%s\n\t\t\t\t\t\tADD:%s\n\t\t\t\t\t\tEMAIL:%s\n\t\t\t\t\t\tPHONENO:%s",coustomer.name,coustomer.add,coustomer.email,coustomer.ph);
  	printf("\n\n\n\t\t\t[---------------------------------------------------------------------------------]");
   	fclose(fp);
   	printf("\nDO YOU WANT TO CONTINUE SHOPPING PRESS 'Y' AND 'N' FOR LOGIN PAGE\n");
	fflush(stdin);
	scanf("%c",&ch);
	if((ch=='Y')||(ch=='y'))
	{
		menu();
					   	 	
	}
	else if((ch=='N')||(ch=='n'))
	{
		login();
	}
}
void oneplus_update_rec() 
{
	char n[30],field[20],newdata[20],ch;
	printf("\n\t\t\t[-------------------------*ONEPLUS 10T 5G CUSTOMER RECORDS*---------------------------]\n");
	FILE *fp,*fp1;
	fp=fopen("OnePlus_coustomer_rec.txt","r+");
	
	if(fp==NULL)
	{
		printf("\nCannot open file\n");
	}
	printf("\n\t\t\t\tENTER PERSON NAME WHOSE RECORD YOU WANT TO UPDATE:");
	scanf("%s",n);
	
	while(fscanf(fp,"%s%s%s%s",coustomer.name,coustomer.add,coustomer.email,coustomer.ph)!=EOF)
	{
		
	
