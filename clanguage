struct project
{ 
 char name[40];
 char add[40];
 char email[40];
 char  ph[20];
 }coustomer;
 
struct login 
{
	char name[20];
	char pass[20];
	
}l;

main()
{
	system("color 3");
	char ch;
	 int n;   
	
	printf("**********************************************************************************************\n");
	printf("***********************************WELCOME TO AMAZON SHOPPING*********************************\n");
	printf("**********************************************************************************************");

	printf("\n\n\n\t\t\t\t[-----------LOGIN PAGE-----------]\n");

	printf("\n\t\t\t\t\t1...SIGN UP");
	
	printf("\n\n\t\t\t\t\t2.....SIGN IN\n");
	printf("\n\t\t\t\t[---------------------------------]\n\n");
	printf("\nSELECT YOUR CHOICE\n");
	scanf("%d",&n);
	if(n==1)
	{
		
	
	 	char u[20],p[20];
		FILE *fp;
		fp=fopen("login1.txt","w");
		printf("\n\t\t\t\t[-------------SIGN UP-------------]\n\n");
		printf("\n\t\t\t\t\t ENTER USERNAME:");
		scanf("%s",l.name);
		printf("\n\t\t\t\t\t ENTER PASSWORD:");
		scanf("%s",l.pass);
	
		fprintf(fp,"\n\n%s \n\n%s",l.name,l.pass);
	    printf("\n\n\t\t\t\t\t*REGISTRATION DONE*\n");
	    printf("\n\t\t\t\t[----------------------------------]\n\n");
	    system("cls");
	
		fclose(fp);
		
		 login1();
    }
	else if(n==2)
	{
		login();
		
	}
	else if(n>=2)
	{
		printf("\nPLEASE ENTER RIGHT LOGIN ....");
	}
   
	

}
void login1()
{
	char ch;
	printf("\nDO U WANT TO SIGN IN RIGHT NOW PRESS 'y'\n ");
	fflush(stdin);
	scanf("%c",&ch);
	if((ch=='Y')||(ch=='y'))
	{
		login();	
	
    }
	
	
	
	
}

void login()
{
			    char u[20],p[20];
			    printf("\n\t\t\t\t[-------------SIGN IN-------------]\n\n");
		        printf("\n\t\t\t\t\t ENTER USERNAME:");
				scanf("%s",u);
				printf("\n\t\t\t\t\t ENTER PASSWORD:");
				scanf("%s",p);
			
				FILE *fp1;
				fflush(stdin);
	
				fp1=fopen("login1.txt","r");
				
				while(fscanf(fp1,"\n %s %s",l.name,l.pass)!=EOF)
				{
					
					if(((strcmp(l.name,u)==0))&&((strcmp(l.pass,p)==0)))
			   		{
			    		printf("\n \n\t\t\t\t\t *LOGIN CORRECT* \n");	
			    		printf("\n\t\t\t\t[---------------------------------]\n\n");
			    	     system("cls");
						menu();
			   	 	}			 		
		    	    else 
			        {
		 	        	printf("\n \n\t\t\t\t \t*LOGIN INCORRECT*\n");	
			        	printf("\n\t\t\t\t[---------------------------------]\n\n");
			             system("cls");
			        	login();
 		   	        }
 		   	    	fclose(fp1);
                }
			
	
		
}
void oneplus_delete_rec()
{
	   char ch,n[30],field[20],newdata[20];
	   
		FILE *fp,*fp1;
		printf("\n\t\t\t[-------------------------*ONEPLUS 10T 5G CUSTOMER RECORDS*---------------------------]\n");
		fp=fopen("OnePlus_coustomer_rec.txt","r+");
	
	if(fp==NULL)
	{
		printf("\nCANNOTN OPEN FILE\n");
	}
	printf("\n\t\t\t\tENTER COUSTOMER NAME WHOSE RECORD YOU WANT TO DELETE:");
	scanf("%s",n);
	
	
	while(fscanf(fp,"%s  \n%s  \n%s  \n%s",coustomer.name,coustomer.add,coustomer.email,coustomer.ph)!=EOF)
	{
		if(strcmp(coustomer.name,n)==0)
		{
				printf("\n\t\t\t\t\t\t*RECORD FOUND*");
			    printf("\n\t\t\t\tname=%s\n\t\t\t\tadd=%s\n\t\t\t\temail=%s\n\t\t\t\tphone=%s\n",coustomer.name,coustomer.add,coustomer.email,coustomer.ph);	
		
				fp1=fopen("OnePlus_coustomer_rec.txt","r+");	
				strcpy(coustomer.name,"                    ");
				strcpy(coustomer.add,"                      ");
				strcpy(coustomer.email,"                    ");
				strcpy(coustomer.ph,"                       ");
				
				fprintf(fp1,"%s%s%s%s\n",coustomer.name,coustomer.add,coustomer.email,coustomer.ph);
		
				printf("\n\t\t\t\t\t\t*RECORD DELETED*");
				printf("\n\n\n\t\t\t[---------------------------------------------------------------------------------]");
				fclose(fp);
    	        fclose(fp1);
				printf("\nDO YOU WANT TO CONTINUE SHOPPING PRESS 'Y' AND 'N' FOR LOGIN PAGE\n");
				fflush(stdin);
				scanf("%c",&ch);
				if((ch=='Y')||(ch=='y'))
				{
						menu();
					   	 	
				}
				else if((ch=='N')||(ch=='n'))
				{
					login();
				}
				
		}
		else
		{
				printf("\n\t\t\t\t\t*RECORD  NOT FOUND*");
				printf("\nDO YOU WANT TO CONTINUE SHOPPING PRESS 'Y' AND 'N' FOR LOGIN PAGE\n");
				fflush(stdin);
				scanf("%c",&ch);
				if((ch=='Y')||(ch=='y'))
				{
						menu();
					   	 	
				}
				else if((ch=='N')||(ch=='n'))
				{
					login();
				}
		}
	}
		
		
    

}
